<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gnt.domain.mapper.MLoginUserMapper">

	<select id="selectLoginUserByLoginId" parameterType="String" resultType="com.gnt.domain.entity.MLoginUserEntity">
		select
			* 
		from m_login_user
		where login_id = #{loginId}
		;
	</select>


</mapper>
